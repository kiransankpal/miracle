<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="InvoiceCopyPrintVO"
  Version="11.1.1.64.93"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false"
  FetchMode="FETCH_AS_NEEDED"
  MaxFetchSize="1">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="model.view.InvoiceCopyPrintVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bind_invoice_id"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery>
    <![CDATA[select encodeddocument, invoice_interface_id 
 from sf_ap_oio_headers_all a
where  a.invoice_interface_id = :bind_invoice_id]]>
  </SQLQuery>
  <ViewAttribute
    Name="Encodeddocument"
    IsUpdateable="false"
    IsQueriable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.BlobDomain"
    ColumnType="BLOB"
    AliasName="ENCODEDDOCUMENT"
    Expression="ENCODEDDOCUMENT"
    SQLType="BLOB"/>
  <ViewAttribute
    Name="InvoiceInterfaceId"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="INVOICE_INTERFACE_ID"
    Expression="INVOICE_INTERFACE_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="39"/>
    </DesignTime>
  </ViewAttribute>
  <ResourceBundle>
    <XliffBundle
      id="model.CommonModelBundle"/>
  </ResourceBundle>
</ViewObject>
