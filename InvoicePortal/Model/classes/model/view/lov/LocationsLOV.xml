from po_vendor_sites_pay_ap_v pov, financials_system_params_all fsp,
     ap_suppliers aps, hz_party_sites hzps
where pov.vendor_id = :vendor_id and pov.active_flag = 'Y' 
  and pov.org_id = fsp.org_id and hzps.party_site_id(+) = pov.party_site_id
  and pov.vendor_id = aps.vendor_id and DECODE(aps.vendor_type_lookup_code,'EMPLOYEE', 'A',NVL(hzps.status, 'I')) = ('A')]]>
