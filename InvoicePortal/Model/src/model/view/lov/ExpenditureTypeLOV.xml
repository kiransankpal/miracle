  <SQLQuery>
    <![CDATA[select 
	expenditure_type, 
	description, 
	unit_of_measure ,
	cost_rate_flag,
	system_linkage_m, 
	system_linkage_function 
from 
	pa_expenditure_types_expend_v 
where 
	system_linkage_function = 'VI' 
and 
	(sysdate between expnd_typ_start_date_active 
and 
	nvl(expnd_typ_end_date_active, sysdate)) 
and 
	(sysdate between sys_link_start_date_active 
and 
	nvl(sys_link_end_date_active,sysdate)) 
order by 
	expenditure_type]]>
  </SQLQuery>
