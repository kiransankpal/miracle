                              minValue="#{pageFlowScope.InvoiceSearchBean.invoiceStartDate}"
                              partialTriggers="inputDate6"
                              disabled="#{pageFlowScope.InvoiceSearchBean.invoiceStartDate eq null ? true : false}">
                  <af:convertDateTime pattern="dd-MM-yy"/>
                </af:inputDate>
              </af:panelGroupLayout>
            </af:gridCell>
