                                 autoSubmit="true">
                <f:selectItem itemValue="Standard" itemLabel="Standard "/>
                <f:selectItem itemLabel="Credit Memo" itemValue="Credit Memo"/>
              </af:selectOneRadio>
            </af:gridCell>
